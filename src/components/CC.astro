---
import PrimateSlide from './PrimateSlide.astro';
import { Image } from 'astro:assets';

// Import CC assets
import ccImage from '../assets/CC/cc.jpg';
import ccAudioFile from '../assets/CC/20241209_131112.wav';
import ccAnnotations from '../assets/CC/20241209_131112.txt?raw';
import ccReference from '../assets/CC/ref.txt?raw';

const subtitle = "Shock headed Capuchin monkey / Mono capuchino de cabeza de choque / Cebus cuscinus";

// Create a modified species object with imported assets
const speciesAssets = {
  image: ccImage,
  audio: ccAudioFile,
  annotations: ccAnnotations,
  reference: ccReference
};
---

<PrimateSlide 
  speciesCode="CC"
  subtitle={subtitle}
  assets={speciesAssets}
  bgClass="bg-gradient-to-b from-black to-gray-900"
/> 